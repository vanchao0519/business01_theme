{#
/**
 * @file
 * Theme override for a breadcrumb trail.
 *
 * Available variables:
 * - breadcrumb: Breadcrumb trail items.
 */
#}
{% if breadcrumb %}
  {% for item in breadcrumb %}
    <li class="breadcrumb-item">
      {% if item.url %}
        <a class="fs-4 fw-bolder text-uppercase" href="{{ item.url }}">{{ item.text }}</a>
      {% else %}
        {{ item.text }}
      {% endif %}
    </li>
  {% endfor %}
{% endif %}
